{
    "name": "codeboxr/writelog",
    "description": "simplified logging for laravel",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/codeboxrcodehub/writelog",
    "keywords": [
        "Laravel",
        "log",
        "writelog"
    ],
    "autoload": {
        "psr-4": {
            "Codeboxr\\Writelog\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Codeboxr",
            "email": "sabuj@codeboxr.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^7.3|^8.0|^8.1",
        "illuminate/support": "~5|~6|~7|~8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codeboxr\\Writelog\\WriteLogServiceProvider"
            ]
        }
    }
}
